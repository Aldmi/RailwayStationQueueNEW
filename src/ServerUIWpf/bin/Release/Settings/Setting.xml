<?xml version="1.0" encoding="utf-8"?>
<Root>
  <LogSetting>
    <PortionString>1</PortionString>
	<CountPortion>1000</CountPortion>
  </LogSetting>

  <Server>
    <Listener>
      <Port>50000</Port>
    </Listener>
	
    <SerialPorts>
		<Serial>
		  <Port>8</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>
		
		<Serial>
		  <Port>9</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>
	</SerialPorts>
  </Server>


	<Queues>
		<Queue Id="1" Name="Main">
			<Prefix Name="К" Priority="7"/>  <!-- 1. Купить билет -->
			<Prefix Name="М" Priority="10"/> <!-- 2. Купить билет в международном сообщении -->
			<Prefix Name="Г" Priority="10"/> <!-- 3. Оформление групп пассажиров-->
			<Prefix Name="И" Priority="10"/> <!-- 4. Оформление маломобильных пассажиров -->
			<Prefix Name="С" Priority="10"/> <!-- 9. Получить справку -->
		</Queue>
		
		<Queue Id="2" Name="Admin">
			<Prefix Name="А" Priority="10"/>  <!-- 12. Администратор -->
		</Queue>
	</Queues>


	<Cashiers>
	   <Cashier Id="1" Port="9" NameQueue="Main" Prefix="С" MaxCountTryHanding="2"/>
	   <Cashier Id="2" Port="9" NameQueue="Main" Prefix="С" MaxCountTryHanding="2"/>
	   <Cashier Id="3" Port="9" NameQueue="Main" Prefix="И,All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="4" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="5" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="6" Port="9" NameQueue="Main" Prefix="All,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="7" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="8" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="9" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="10" Port="9" NameQueue="Admin" Prefix="А" MaxCountTryHanding="2"/>
	   <Cashier Id="11" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="12" Port="9" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="13" Port="8" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="14" Port="8" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>
	   <Cashier Id="15" Port="8" NameQueue="Main" Prefix="All,С,М,Г" MaxCountTryHanding="2"/>  
	   <Cashier Id="16" Port="8" NameQueue="Main" Prefix="Г,All,С,М" MaxCountTryHanding="2"/>
	   <Cashier Id="17" Port="8" NameQueue="Main" Prefix="М,All,Г,С" MaxCountTryHanding="2"/>
	</Cashiers>
</Root>