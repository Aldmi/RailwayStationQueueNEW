<Root>
  <LogSetting>
    <PortionString>1</PortionString>
	<CountPortion>1000</CountPortion>
  </LogSetting>

  <Server>
    <Listener>
      <Port>50000</Port>
    </Listener>
	
    <SerialPorts>
		<Serial>
		  <Port>1</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>500</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>		
	</SerialPorts>
  </Server>


	<Queues>
		<Queue Id="1" Name="Main">
			<Prefix Name="А" Priority="10"/>  <!-- 1. Купить билет оформить возврат -->
