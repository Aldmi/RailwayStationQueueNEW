<?xml version="1.0" encoding="utf-8"?>
<Root>
  <LogSetting>
    <PortionString>1</PortionString>
	<CountPortion>1000</CountPortion>
  </LogSetting>

  <Server>
    <Listener>
      <Port>50000</Port>
    </Listener>
	
    <SerialPorts>
		<Serial>
		  <Port>8</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>

		<Serial>
		  <Port>9</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>
	</SerialPorts>
  </Server>

<!-- Очереди: -->
<!-- 1. Получить справку - префикс "C"                                    -  -->
<!-- 2. Купить билет - префикс "T" -->
