<?xml version="1.0" encoding="utf-8"?>
<Root>
  <LogSetting>
    <PortionString>1</PortionString>
	<CountPortion>1000</CountPortion>
  </LogSetting>

  
  <Server>
    <Listener>
      <Port>50000</Port>
    </Listener>
	
    <SerialPorts>
		<Serial>
		  <Port>8</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>

		<Serial>
		  <Port>9</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>
	</SerialPorts>
  </Server>

  
  <Queues>
    <Queue Id="1" Name="Main">
	    <Prefix Name="M" Priority="5"/>
		<Prefix Name="A" Priority="9"/>
	</Queue>
  </Queues>
  
  
<!-- Очереди: -->
<!-- 1. Купить билет - префикс "К" --->
<!-- 2. Купить билет меж государственного сообщения - префикс "M" -->
<!-- 3. Оформление групп пассажиров                 - префикс "Т" -->
<!-- 4. Оформление маломобильных пассажиров         - префикс "Н" -->
<!-- 5. Возврат                                     - префикс "В" -->
<!-- 6. Переоформление                              - префикс "Р" -->
<!-- 7. Восстановление билетов                      - префикс "С" -->
<!-- 8. Замена персональных данных                  - префикс "Е" -->
<!-- 9. Получить справку                            - префикс "" -->
  
    <Cashiers>
	   <Cashier Id="1" Port="9" Prefix="C" MaxCountTryHanding="3"/>
	   <Cashier Id="2" Port="9" Prefix="C" MaxCountTryHanding="3"/>
	   <Cashier Id="3" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="4" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="5" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="6" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="7" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="8" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="9" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="10" Port="9" Prefix="A" MaxCountTryHanding="3"/>
	   <Cashier Id="11" Port="9" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="12" Port="9" Prefix="T" MaxCountTryHanding="3"/>

	   <Cashier Id="13" Port="8" Prefix="M" MaxCountTryHanding="3"/>
	   <Cashier Id="14" Port="8" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="15" Port="8" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="16" Port="8" Prefix="T" MaxCountTryHanding="3"/>
	   <Cashier Id="17" Port="8" Queue="Main" Prefix="M,All" MaxCountTryHanding="3"/>
    </Cashiers>
</Root>