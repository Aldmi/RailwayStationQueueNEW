<?xml version="1.0" encoding="utf-8"?>
<Root>
  <LogSetting>
    <PortionString>1</PortionString>
	<CountPortion>1000</CountPortion>
  </LogSetting>

  <Server>
    <Listener>
      <Port>50000</Port>
    </Listener>
	
    <SerialPorts>
		<Serial>
		  <Port>4</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>1000</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>	
	</SerialPorts>
  </Server>


	<Queues>
		<Queue Id="1" Name="Main">
			<Prefix Name="К" Priority="7"/>  <!-- 1. Купить билет -->
			<Prefix Name="М" Priority="10"/> <!-- 2. Купить билет в международном сообщении -->
			<Prefix Name="Г" Priority="10"/> <!-- 3. Оформление групп пассажиров-->
			<Prefix Name="И" Priority="10"/> <!-- 4. Оформление маломобильных пассажиров -->
			<Prefix Name="В" Priority="7"/>  <!-- 5. Возврат -->
			<Prefix Name="П" Priority="7"/>  <!-- 6. Переоформление -->
			<Prefix Name="У" Priority="9"/>  <!-- 7. Восстановление утеренных, испорченных билетов -->
			<Prefix Name="З" Priority="8"/>  <!-- 8. Замена персональных данных-->
			<Prefix Name="С" Priority="10"/> <!-- 9. Получить справку -->
			<Prefix Name="Б" Priority="7"/>  <!-- 10.Оформить Багаж/живность -->
		</Queue>
		
		<Queue Id="2" Name="Admin">
			<Prefix Name="А" Priority="10"/>  <!-- 12. Администратор -->
		</Queue>
	</Queues>


	
	<Cashiers>	`
	   <Cashier Id="13" Port="4" NameQueue="Main" Prefix="All" MaxCountTryHanding="2"/>
	</Cashiers>
</Root>