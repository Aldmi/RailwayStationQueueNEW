			<Prefix Name="В" Priority="10"/>  <!-- 3. Оформление по воинским перевозочным документам -->
			<Prefix Name="Б" Priority="10"/> <!-- 4. Оформление багажа и животных/выдача платных справок-->
			<Prefix Name="С" Priority="10"/> <!-- 5.Участник СВО -->
		</Queue>
		
		<Queue Id="2" Name="Admin">
			<Prefix Name="А" Priority="10"/>  <!-- 9. Администратор  -->
		</Queue>
	</Queues>


	
