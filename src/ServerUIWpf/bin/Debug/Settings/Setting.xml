<?xml version="1.0" encoding="utf-8"?>
<Root>
  <LogSetting>
    <PortionString>1</PortionString>
	<CountPortion>1000</CountPortion>
  </LogSetting>

  <Server>
    <Listener>
      <Port>50000</Port>
    </Listener>
	
    <SerialPorts>
		<Serial>
		  <Port>1</Port>
		  <BaudRate>57600</BaudRate>
		  <DataBits>8</DataBits>
		  <StopBits>1</StopBits>
		  <TimeRespon>250</TimeRespon>
		  <TimeCycleReConnect>3000</TimeCycleReConnect>
		</Serial>
	</SerialPorts>
  </Server>


	<Queues>
		<Queue Id="1" Name="Main">
			<Prefix Name="К" Priority="5"/>  <!-- 1. Купить билет -->
			<Prefix Name="М" Priority="6"/>  <!-- 2. Купить билет в международном сообщении -->
			<Prefix Name="Г" Priority="7"/>  <!-- 3. Оформление групп пассажиров-->
			<Prefix Name="Н" Priority="10"/> <!-- 4. оформление маломобильных пассажиров -->
			<Prefix Name="В" Priority="8"/>  <!-- 5. Возврат -->
			<Prefix Name="П" Priority="8"/>  <!-- 6. Переоформление -->
			<Prefix Name="У" Priority="8"/>  <!-- 7. Восстановление утеренных, испорченных билетов -->
			<Prefix Name="З" Priority="7"/>  <!-- 8. Замена персональных данных-->
			<Prefix Name="С" Priority="4"/>  <!-- 9. Получить справку -->
			<Prefix Name="Б" Priority="5"/>  <!-- 10. Оформить Багаж/живность -->
		</Queue>
		
		<Queue Id="2" Name="Admin">
			<Prefix Name="А" Priority="4"/>  <!-- 12. Администратор -->
		</Queue>
	</Queues>


	<Cashiers>
	   <Cashier Id="1" Port="1" NameQueue="Main" Prefix="Г,M,All" MaxCountTryHanding="3"/>
	   <!-- <Cashier Id="2" Port="1" Queue="Main" Prefix="M,All" MaxCountTryHanding="3"/> -->
	</Cashiers>
</Root>